{% stylesheet %}
.containerAZ {
    width: 96.2%;
    height: 650px;
    border: 1px solid transparent;
    margin: 30px;
    display: grid;
    grid-template-columns: 1fr 2fr 1fr;
    grid-template-rows: 1fr 1fr 1fr 1fr;
    grid-gap: 20px 20px;
}

.div1AZ{
    overflow: hidden;
  border:1px solid #000;
}

.div1AZ:nth-child(1) {
    background-color: red;
    grid-column-start: 1;
    grid-column-end: 2;
    grid-row-start: 1;
    grid-row-end: 3;
    position: relative;
    color: #000;
    box-sizing: border-box;
}

#img1AZ {
    width: 100%;
    height: 100%;
    position: absolute;
    object-fit: cover;
    object-position: 100% 100%;
}

.paraAZ1{
    position: absolute;
    font-size: 18px;
    left: 1.9vw;
    /* top: 0.8vw; */
    line-height: 1.3;
}

.paraAZ2{
    position: absolute;
    font-family: Arial, Helvetica, sans-serif;
    font-size: 18px;
    bottom: .4vw;
    left: 1.9vw;
    color: #525050;
    line-height: 1.2;
}

.paraAZ {
    width: 100%;
    background-image: linear-gradient(to right, #fff, #3A4B5B);
    position: absolute;
    /* bottom: -66px; */
    bottom: 0px;
    font-family: Arial, Helvetica, sans-serif;
    font-size: 20px;
    text-align: center;
    transition: bottom 1s;
}

/* .div1AZ:hover .paraAZ{
    bottom: 0px;
} */

.div1AZ:nth-child(2) {
    background-color: #E9EEFA;
    grid-column-start: 2;
    grid-column-end: 3;
    grid-row-start: 1;
    grid-row-end: 5;
    position: relative;
    overflow: hidden;
    box-sizing: border-box;
    color: #000;
}

#img2AZ {
    width: 400px;
    height: auto;
    position: absolute;
    top: 12%;
    right: 6vw;
    object-fit: cover;
}

.goldivAZ {
    width: 250px;
    height: 250px;
    background-color: #CDDCFF;
    position: absolute;
    right: -20px;
    top: -20px;
    border-radius: 50%;
}

.div1AZ:nth-child(3) {
    background-color: purple;
    grid-column-start: 3;
    grid-column-end: 4;
    grid-row-start: 1;
    grid-row-end: 3;
    position: relative;
    box-sizing: border-box;
    color: #000;
}

.div1AZ:nth-child(4) {
    background-color: pink;
    grid-column-start: 1;
    grid-column-end: 2;
    grid-row-start: 3;
    grid-row-end: 5;
    box-sizing: border-box;
    position: relative;
    color: #000;
}

.div1AZ:nth-child(5) {
    background-color: hotpink;
    grid-column-start: 3;
    grid-column-end: 4;
    grid-row-start: 3;
    grid-row-end: 5;
    box-sizing: border-box;
    position: relative;
    color: #000;
}
