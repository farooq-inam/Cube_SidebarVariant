@media screen and (max-width: 1450px) {
    .containerAZ {
        grid-gap: 30px 25px;
    }
}

@media screen and (max-width: 1267px) {
    .containerAZ {
        height: 600px;
        grid-gap: 20px 15px;
    }
}

@media screen and (max-width: 1222px) {
    .containerAZ {
        grid-gap: 15px 10px;
        height: 500px;
        margin: 20px;
    }

    #img2AZ {
        width: 300px;
        height: auto;
    }
}

@media screen and (max-width: 1040px) {
    .containerAZ {
        height: 1300px;
        grid-gap: 20px 10px;
        grid-template-columns: 1fr 2px 1fr;
        grid-template-rows: 1fr 1fr 1fr 1fr 1fr 1fr 1fr 1fr 1fr 1fr 1fr;
    }

    .div1AZ:nth-child(1) {
        grid-column-start: 1;
        grid-column-end: 2;
        grid-row-start: 1;
        grid-row-end: 4;
    }

    .div1AZ:nth-child(2) {
        grid-column-start: 1;
        grid-column-end: 4;
        grid-row-start: 4;
        grid-row-end: 9;
        z-index: 1;
    }

    #img2AZ {
        width: 400px;
        height: auto;
    }

    .div1AZ:nth-child(3) {
        grid-column-start: 3;
        grid-column-end: 4;
        grid-row-start: 1;
        grid-row-end: 4;
    }

    .div1AZ:nth-child(4) {
        background-color: pink;
        grid-column-start: 1;
        grid-column-end: 2;
        grid-row-start: 9;
        grid-row-end: 12;
    }

    .div1AZ:nth-child(5) {
        background-color: hotpink;
        grid-column-start: 3;
        grid-column-end: 4;
        grid-row-start: 9;
        grid-row-end: 12;
    }

    .start{
        width: 90%;
        height: 1000px;
        grid-template-columns: 1fr 1fr 1fr;
        grid-template-rows: 1fr 1fr 1fr 1fr;
    }
}

@media screen and (max-width: 891px) {
    .start{
        width: 65%;
        height: 1400px;
        grid-template-columns: 1fr 1fr;
        grid-template-rows: 1fr 1fr 1fr 1fr 1fr 1fr;
        grid-gap: 30px;
    }
}

@media screen and (max-width: 718px) {
    .start{
        width: 75%;
    }
}

@media screen and (max-width: 700px) {
    .containerAZ {  
        height: auto;
        margin: 10px;
        grid-template-columns: 1fr;
        grid-template-rows: 400px 400px 600px 400px 400px;
    }

    .div1AZ:nth-child(1) {
        background-color: red;
        grid-column-start: 1;
        grid-column-end: 2;
        grid-row-start: 1;
        grid-row-end: 2;
        overflow: hidden;
    }

    .div1AZ:nth-child(2) {
        background-color: #E9EEFA;
        grid-column-start: 1;
        grid-column-end: 2;
        grid-row-start: 3;
        grid-row-end: 4;
    }

    #img2AZ {
